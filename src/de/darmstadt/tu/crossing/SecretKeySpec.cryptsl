SPEC javax.crypto.spec.SecretKeySpec
USES_OBJECTS 
	java.lang.String alg;
	byte[] keyMaterial;
	int len;
	int off;
		
REQUIRED_EVENTS
	c1: SecretKeySpec(keyMaterial, alg);
	c2: SecretKeySpec(keyMaterial, off, len, alg);
		
	Cons := c1, c2;
ENFORCE_ORDER
 	Cons
ENFORCES_CONSTRAINTS
	random(keyMaterial);
	keyMaterial.size >= off + len
ENSURES
	Keyspec(this, alg, keylength);