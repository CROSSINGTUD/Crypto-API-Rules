SPEC javax.crypto.spec.SecretKeySpec
OBJECTS 
	java.lang.String alg;
	byte[] keyMaterial;
	int len;
	int off;
		
EVENTS
	c1: SecretKeySpec(keyMaterial, alg);
	c2: SecretKeySpec(keyMaterial, off, len, alg);
		
	Cons := c1, c2;
ORDER
 	Cons
CONSTRAINTS
	random[keyMaterial];
	keyMaterial.size >= off + len;
ENSURES
	keySpec[this, _];
	generatedKey[this, alg];