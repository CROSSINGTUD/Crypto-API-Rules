/*
 * A CrySL specification for using the Hybrid Encryption modes 
 * of the Google Tink library. 
 */
 SPEC com.google.crypto.tink.HybridDecrypt

OBJECTS
	byte[] plainText;
	byte[] cipherText;
	byte[] context;

EVENTS
	dec : plainText = decrypt(cipherText, context);
	
	Mode := dec ; 

ORDER
	(Mode)*

REQUIRES 
	hybridDecryptPrimitive[this];