SPEC org.bouncycastle.crypto.io.SignerOutputStream

OBJECTS
	org.bouncycastle.crypto.Signer signer;
	byte[] byte;
	int off;
	int len;
	int b;

EVENTS
	c : SignerOutputStream(signer);
	
	r1: write(b); 
	r2: write(byte, off, len);
	Writes:= r1 | r2;
	
ORDER
	c, Writes+
	
CONSTRAINTS
	len > off;
	
ENSURES
	signedOutputStream[this, signer]; // to be verified