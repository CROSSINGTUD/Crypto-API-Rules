SPEC org.bouncycastle.crypto.engines.AESLightEngine

OBJECTS
	org.bouncycastle.crypto.CipherParameters params;
	byte[] buf;
	byte[] buf2;
	
EVENTS
	c : AESLightEngine();	
	
	i : init(_, params);
	
	p : processBlock(buf, _, buf2, _);
	
ORDER
	c , (i+, p*)?	// there cannot be processBlock without init	
	
REQUIRES
	generatedCipherParameters[params];	
	
ENSURES
	generatedEngine[this];
	encrypted[buf] after p;