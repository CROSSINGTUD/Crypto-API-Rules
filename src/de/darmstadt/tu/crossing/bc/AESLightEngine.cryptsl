SPEC org.bouncycastle.crypto.engines.AESLightEngine

OBJECTS
	org.bouncycastle.crypto.CipherParameters params;
	byte[] buf;
	
EVENTS
	c : AESLightEngine();	
	
	i : init(_, params);
	
	p : processBlock(buf, _, buf, _);
	
ORDER
	c | (c, i+, p*)	// there cannot be processBlock without init	
	
REQUIRES
	generatedKey[params];	
	
ENSURES
	generatedEngine[this];
	encrypted[buf] after p;