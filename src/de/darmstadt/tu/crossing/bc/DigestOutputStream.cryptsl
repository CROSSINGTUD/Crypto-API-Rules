SPEC org.bouncycastle.crypto.io.DigestOutputStream
OBJECTS
	org.bouncycastle.crypto.Digest md;
	int b;
	byte[] bArr;
	int off;
	int len;
	byte[] res;

EVENTS
	c1 : DigestOutputStream(md);
	Cons := c1;
	
	w1 : write(b); 
	w2 : write(bArr, off, len);
	Writes := w1 | w2;
	
	g1 : res = getDigest();
	Gets := g1;
	
ORDER
	c, Writes+, Gets
	
CONSTRAINTS
	len > off;
	
ENSURES
	digestedOutputStream[this, md]; // to be verified