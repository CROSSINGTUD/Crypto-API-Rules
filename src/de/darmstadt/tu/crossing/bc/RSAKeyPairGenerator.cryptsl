SPEC org.bouncycastle.crypto.generators.RSAKeyPairGenerator

OBJECTS
	org.bouncycastle.crypto.params.RSAKeyGenerationParameters genParam;
//	org.bouncycastle.crypto.AsymmetricCipherKeyPair pair;

EVENTS
	c : RSAKeyPairGenerator();
	
	i : init(genParam);
	
	g : generateKeyPair();
//	g : pair = generateKeyPair();
	
ORDER
	c, (i, g)+
	
REQUIRES
	generatedRSAKeyParam[genParam];
	
ENSURES
	generatedRSAKeyPair[this] after g;
//	generatedRSAKeyPair[pair];