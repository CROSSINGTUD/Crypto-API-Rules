SPEC org.bouncycastle.crypto.AsymmetricCipherKeyPairGenerator
OBJECTS
	org.bouncycastle.crypto.KeyGenerationParameters params;
	org.bouncycastle.crypto.AsymmetricCipherKeyPair kp;

EVENTS
	i: init(params);
	k: kp = generateKeyPair();

ORDER
	i, k
	
REQUIRES
	generatedKeyGenParams[params];

ENSURES
	generatedKeyPair[kp] after k;