SPEC org.bouncycastle.crypto.generators.DHBasicKeyPairGenerator
OBJECTS
	org.bouncycastle.crypto.KeyGenerationParameters params;
	org.bouncycastle.crypto.AsymmetricCipherKeyPair kp;

EVENTS
	i: init(params);
	k: kp = generateKeyPair();

ORDER
	i, k
	
REQUIRES
	generatedDHKeyGenParams[params];

ENSURES
	generatedDHKeyPair[kp] after k;