SPEC org.bouncycastle.crypto.io.MacOutputStream

OBJECTS
	org.bouncycastle.crypto.Mac mac;
	byte[] byte;
	int off;
	int len;
	int b;

EVENTS
	c : MacOutputStream(mac);
	
	r1: write(b); 
	r2: write(byte, off, len);
	Writes:= r1 | r2;
	
ORDER
	c, Writes+
	
CONSTRAINTS
	len > off;
	
ENSURES
	maccedOutputStream[this, mac]; // to be verified