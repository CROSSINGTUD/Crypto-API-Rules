SPEC org.bouncycastle.crypto.io.DigestInputStream
OBJECTS
	java.io.InputStream is;
	org.bouncycastle.crypto.Digest md;
	byte[] b;
	int off;
	int len;

EVENTS
	c: DigestInputStream(is, md);
	
	r1: read(); 
	r2: read(b, off, len);
	Reads:= r1 | r2;
	
ORDER
	c, Reads+
	
CONSTRAINTS
	len > off;
	
ENSURES
	digestedInputStream[is, md];