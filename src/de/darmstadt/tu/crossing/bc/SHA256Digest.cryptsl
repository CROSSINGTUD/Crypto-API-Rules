SPEC org.bouncycastle.crypto.digests.SHA256Digest

OBJECTS
	byte input;
	byte[] inputArray;
	int inputOff;
	byte[] outputArray;
	int outputOff;
	int len;
	
EVENTS
	c1 : SHA256Digest();
	Cons := c1;
	
	u1 : update(input);
	u2 : update(inputArray, inputOff, len);
	Updates := u1 | u2;
	
	f : doFinal(outputArray, outputOff);
	Finals := f;
	
ORDER
	Cons, Updates+, Finals
	
CONSTRAINTS
	len > inputOff;
	
ENSURES
	digested[outputArray];
	