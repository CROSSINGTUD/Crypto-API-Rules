SPEC javax.crypto.spec.IvParameterSpec

OBJECTS 
	byte[] iv;
	int offset;
	int len;
	
EVENTS
	c1: IvParameterSpec(iv);
	c2: IvParameterSpec(iv, offset, len);
	Con := c1 | c2;
	
ORDER
	Con
	
CONSTRAINTS
	length[iv] >= offset + len;
	offset >= 0;
	len > 0;
	
REQUIRES
	randomized[iv];

ENSURES
	preparedIV[this];
