SPEC javax.crypto.CipherOutputStream
OBJECTS
	java.io.OutputStream os;
	javax.crypto.Cipher ciph;
	byte[] data;
	int off;
	int len;
	int b;
	
EVENTS
	c1: CipherOutputStream(os, ciph);
	Constructs:= c1;
	
	w1: write(b);
	w2: write(data);
	w3: write(data, off, len);
	Writes:= w1 | w2 | w3;
	
	c: close();
	
ORDER
	Constructs, Writes+, c
	
CONSTRAINTS
	len > off;
	
ENSURES
	cipheredOutputStream[os, ciph];