SPEC org.bouncycastle.crypto.AsymmetricBlockCipher

OBJECTS
	org.bouncycastle.crypto.CipherParameters params;
	boolean is_encryption;
	byte[] input;
	int input_off;
	int len;
	
EVENTS
	
	i : init(is_encryption, params);
	
	p : processBlock(input, input_off, len);
	
ORDER
	i, p*
	
REQUIRES
	generatedEngineOrMode[this];
	generatedRSAKeyParams[params]; //either from RSAKeyParameters or RSAPrivateCrtKeyParameters
 