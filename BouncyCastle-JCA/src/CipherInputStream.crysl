SPEC javax.crypto.CipherInputStream

OBJECTS
	java.io.InputStream is;
	javax.crypto.Cipher ciph;
	byte[] b;
	int off;
	int len;
	
EVENTS
	c1: CipherInputStream(is);
	c2: CipherInputStream(is, ciph);
	Cons:= c1 | c2;
	
	r1: read();
	r2: read(b); 
	r3: read(b, off, len);
	Reads:= r1 | r2 | r3;
	
	Close: close();
	
ORDER
	Cons, Reads+, Close

CONSTRAINTS
	len > off;
	
REQUIRES
	generatedCipher[ciph];
	
ENSURES
	cipheredInputStream[is, ciph];
	