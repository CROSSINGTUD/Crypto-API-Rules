SPEC org.bouncycastle.crypto.io.MacInputStream

OBJECTS
	java.io.InputStream is;
	org.bouncycastle.crypto.Mac mac;
	byte[] b;
	int off;
	int len;
	
EVENTS
	c : MacInputStream(is, mac);
	
	r1 : read();
	r2 : read(b, off, len);
	Reads:= r1 | r2;
	
ORDER
	c, Reads+
	
CONSTRAINTS
	len > off;
	
ENSURES
	maccedInputStream[is, mac];