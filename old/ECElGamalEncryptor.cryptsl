SPEC org.bouncycastle.crypto.ec.ECElGamalEncryptor

OBJECTS
	org.bouncycastle.crypto.CipherParameters keyParams;
	org.bouncycastle.math.ec.ECPoint data;
	org.bouncycastle.crypto.ec.ECPair cipherText;

EVENTS
	c1 : ECElGamalEncryptor();
	Cons := c1;
	
	i1 : init(keyParams);
	Inits := i1;
	
	e1 : cipherText = encrypt(data);
	Encrypts := e1;

ORDER
	Cons, (Inits, Encrypts+)+

REQUIRES
	generatedCipherParams[keyParams];

ENSURES
	generatedGamalEncryptor[cipherText] after e1;