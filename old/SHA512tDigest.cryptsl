SPEC org.bouncycastle.crypto.digests.SHA512tDigest

OBJECTS
	org.bouncycastle.crypto.digests.SHA512tDigest digest;
	byte[] edigest;
	byte input;
	byte[] inputArray;
	int inputOff;
	byte[] outputArray;
	int outputOff;
	int len;
	
EVENTS
	c1 : SHA512tDigest();
	c2 : SHA512tDigest(digest);
	c3 : SHA512tDigest(edigest);
	Cons := c1 | c2 | c3;
	
	u1 : update(input);
	u2 : update(inputArray, inputOff, len);
	Updates := u1 | u2;
	
	f : doFinal(outputArray, outputOff);
	Finals := f;
	
	r : reset();
	
ORDER
	Cons, Updates+, Finals
	
CONSTRAINTS
	len > inputOff;
	
REQUIRES
	generatedDigest[digest];
	
ENSURES
	generatedDigest[this] after Cons;
	digested[outputArray];
	