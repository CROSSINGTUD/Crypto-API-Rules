SPEC org.bouncycastle.crypto.modes.OFBBlockCipher

OBJECTS
	org.bouncycastle.crypto.BlockCipher engine;
	int blockSize;
	
EVENTS
	c : OFBBlockCipher(engine, blockSize);
	
ORDER
	c
	
CONSTRAINTS
	blockSize in {8, 16, 32, 64, 128}; // size should be in multiples of 8

REQUIRES
	generatedEngineOrMode[engine];
	generatedEngine[engine];
	
ENSURES
	generatedEngineOrMode[this];
