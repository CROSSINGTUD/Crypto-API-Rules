SPEC org.bouncycastle.crypto.io.SignerInputStream

OBJECTS
	java.io.InputStream is;
	org.bouncycastle.crypto.Signer signer;
	byte[] b;
	int off;
	int len;

EVENTS
	c : SignerInputStream(is, signer);
	
	r1: read(); 
	r2: read(b, off, len);
	Reads:= r1 | r2;
	
ORDER
	c, Reads+
	
CONSTRAINTS
	len > off;
	
ENSURES
	signedInputStream[is, signer];