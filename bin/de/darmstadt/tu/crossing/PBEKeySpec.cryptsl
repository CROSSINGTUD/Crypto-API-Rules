SPEC javax.crypto.spec.PBEKeySpec
OBJECTS 
	char[] pw;
	byte[] salt;
	int it;
	int keylength; 
FORBIDDEN
	PBEKeySpec(char[]) => c1;
	PBEKeySpec(char[],byte[],int) => c1;
	
EVENTS
	c1: PBEKeySpec(pw, salt, it, keylength);
	
	cP: clearPassword();
	
ORDER
 	c1,  cP
CONSTRAINTS
	it > 1000;
	neverTypeOf(pw, java.lang.String) ;

REQUIRES
	random[salt];	
ENSURES
	keyspec[this, keylength] after c1;
NEGATES
	keyspec[this, _] after cP;